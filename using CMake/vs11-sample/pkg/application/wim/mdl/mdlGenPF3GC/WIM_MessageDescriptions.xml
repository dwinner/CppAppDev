<SystemConfiguration xmlns:xsd="http://www.w3.org/2001/XMLSchema"
                     xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                     xsi:noNamespaceSchemaLocation="..\..\20_XSD\HMI\SystemConfiguration.xsd">
  <VirtualKeys>       
    <VirtualKey Name="MsgId_WIM_DisplaySlack" />
    <!-- Used by WIM GC to activate slack -->
    <VirtualKey Name="WIM_DisplayWarning" />
    <!-- Used by WIM GC to activate warning -->
    <VirtualKey Name="WIM_DispWarningStatus" />
    <!-- Send if a new warning gets active -->
    <VirtualKey Name="WIM_DispWarningNone" />
    <!-- Send if no warning is active anymore -->
    <VirtualKey Name="MsgId_WIM_SetCtrlrVisibility" />
    <!-- Send to inform WIM strategy about changed visibility -->
    <VirtualKey Name="MsgId_WIMFaultMemUpdate" />
    <!-- Send to update the fault memory displayed warning -->
    <VirtualKey Name="MsgID_WIM_DispSlackStatusOff" />
    <!-- Send if a slack becomes inactive -->
    <VirtualKey Name="MsgId_WIM_PrivacyPopUp_Confirmed" />
    <!-- Send when the privace pop up warning has been confirmed with Yes -->
    <VirtualKey Name="MsgId_WIM_PrivacyPopUp_Reject" />
    <!-- Send when the privace pop up warning has been rejected with No -->
    <VirtualKey Name="MsgId_WIM_MBCallPressed" />
    <!-- Send when the call ended -->
    <VirtualKey Name="MsgId_WIM_PrivacyPopUp_NoPopup"/>
    <!-- Send when the privacy pop up warning has been turned off with Back -->
  </VirtualKeys>
</SystemConfiguration>
